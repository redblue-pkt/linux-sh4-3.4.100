	bool "B2064A Board"
	help
	  B2064A Board for CUT1.0

config SH_ST_B2064B_BOARD
	bool "B2064B Board"
	help
	  B2064B Board for CUT1.0

config SH_ST_B2064C_BOARD
	bool "B2064C Board"
	help
	  B2064C Board for CUT2.0
endchoice

endmenu

endif
